{
  "name": "vk-autocomment",
  "version": "2.1.0",
  "description": "VK Auto Comment Bot",
  "main": "src/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "start": "cross-env NODE_ENV=production node src/index.js",
    "dev": "cross-env NODE_ENV=development DEBUG=vk-autocomment:* node --nolazy src/index.js",
    "lint": "eslint ."
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "cross-env": "^5.2.1",
    "debug": "^4.1.1",
    "readline-sync": "^1.4.10",
    "vk-io": "^4.0.0-rc.29",
    "fs-extra": "^8.1.0"
  },
  "devDependencies": {
    "eslint": "6.6.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "3.1.0",
    "nexe": "4.0.0-beta.3",
    "upx": "^1.0.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  },
  "engines": {
    "node": "^8 || ^10 || ^11 || ^12 || ^13"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aeonixlegit/vk-autocomment.git"
  },
  "keywords": [
    "vk",
    "auto",
    "comment",
    "like",
    "bot",
    "node",
    "js",
    "nodejs",
    "vkauto"
